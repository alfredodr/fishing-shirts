{
  "name": "fishingshirts",
  "version": "1.0.0",
  "description": "Fishing Shirts Now Website",
  "main": "server.js",
  "scripts": {
    "start": "nodemon backend/server",
    "server": "npm run start --prefix backend",
    "client": "npm run dev --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "npm run data:import  --prefix backend",
    "checkDuplicates": "npm run checkDuplicates  --prefix backend",
    "validateData": "npm run validateData  --prefix backend",
    "checkRedirects": "npm run checkRedirects  --prefix backend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alfredodr/fishing-shirts.git"
  },
  "author": "Alfredo Dominguez",
