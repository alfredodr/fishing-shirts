  "bugs": {
    "url": "https://github.com/alfredodr/fishing-shirts/issues"
  },
  "homepage": "https://github.com/alfredodr/fishing-shirts#readme",
  "dependencies": {
    "concurrently": "^8.0.1"
  }
}
